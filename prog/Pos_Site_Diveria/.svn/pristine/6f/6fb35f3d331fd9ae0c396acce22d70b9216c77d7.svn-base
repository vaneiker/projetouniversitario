/*CSS POP UP*/
.closeX {
    -webkit-border-radius: 50%;
    -webkit-box-shadow: 0 1px 5px 0 #949494;
    background: #ff0000;
    border-radius: 50%;
    border: 1px solid #fff;
    box-shadow: 0 1px 5px 0 #949494;
    color: #fff !important;
    font-size: 2rem !important;
    font-weight: 800 !important;
    height: 50px;
    line-height: 45px !important;
    margin: 3px 3px 0px;
    position: absolute !important;
    right: -40px !important;
    text-align: center;
    top: -50px !important;
    width: 50px;
}


.cPopup {
    background: #FFFFFF;
    min-width: 300px;
    max-width: 500px;
    min-height: 200px;
    position: absolute;
    top: 30%;
    left: 55%;
    -webkit-border-radius: 20px;
    border-radius: 20px;
	border: 1px solid #002060;
}

.cPopup p{ font-size:1em; font-weight:600; text-align:center; color:#002060;}
.modal>footer, .modal>header {
    border-radius: 0px !important;
    line-height: 1.45rem;
    padding: 5px;
    background: none;
    color: #555;
    margin: 0px !important;
    position: relative;
    min-height: 40px;
}

    


