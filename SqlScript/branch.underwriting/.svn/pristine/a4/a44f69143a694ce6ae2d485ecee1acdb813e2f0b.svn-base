/*----------BOTONES GENERALES----------*/
.boton_next, .boton_prev {
    display: block;
    position: relative;
    text-transform: none;
    float: right;
    -webkit-border-radius: 20px;
    -moz-border-radius: 20px;
    border-radius: 20px;
    text-align: right !important;
    line-height: 28px;
    border-width: 3px;
    border-style: solid;
    border-color: #02577B;
    height: 35px;
    margin-top: 5px;
    color: #FFF;
    font-weight: bold;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 13px;
    padding-left: 20px;
    padding-right: 35px;
    cursor: pointer;
    background: #00b2fc;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #00b2fc 0%, #02577b 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00b2fc), color-stop(100%,#02577b));
    background: -webkit-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -o-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: -ms-linear-gradient(top, #00b2fc 0%,#02577b 100%);
    background: linear-gradient(to bottom, #00b2fc 0%,#02577b 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00b2fc', endColorstr='#02577b',GradientType=0 );
}

.boton_prev {
    float: left;
    padding-right: 20px;
    padding-left: 35px;
}

    .boton_next div, .boton_prev div {
        display: block;
        padding: 0;
        margin: 0;
        position: absolute;
        top: -3px;
        right: -3px;
        width: 35px;
        height: 35px;
        z-index: 55;
        background: #f4f4f4 url(../images/ico_new/btn/next_icon.png) no-repeat 2px center;
        background-size: 25px 26px;
        -webkit-border-radius: 20px;
        -moz-border-radius: 20px;
        border-radius: 20px;
        border-width: 3px;
        border-style: solid;
        border-color: #02577B;
    }

    .boton_prev div {
        left: -3px;
        background-image: url(../images/ico_new/btn/prev_icon.png);
        background-position: -1px center;
    }

.boton_next div {
    background-position: 0px center;
}

